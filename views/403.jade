doctype html
html
  head
    // Meta, title, CSS, favicons, etc.
    meta(charset='utf-8')
    title Fusion - A Responsive HTML5 Admin UI Template Theme
    meta(name='keywords', content='HTML5 Bootstrap 3 Admin Template UI Theme')
    meta(name='description', content='Fusion - A Responsive HTML5 Admin UI Template Theme')
    meta(name='author', content='AdminDesigns')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    // Font CSS (Via CDN)
    link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800')
    // Bootstrap CSS
    link(rel='stylesheet', type='text/css', href='/vendor/bootstrap/css/bootstrap.min.css')
    // Page CSS
    link(rel='stylesheet', type='text/css', href='/css/vendor/fonts/icomoon/icomoon.min.css')
    // Theme CSS
    link(rel='stylesheet', type='text/css', href='/css/vendor.css')
    link(rel='stylesheet', type='text/css', href='/css/theme.css')
    link(rel='stylesheet', type='text/css', href='/css/utility.css')
    link(rel='stylesheet', type='text/css', href='/css/custom.css')
    // Favicon
    link(rel='shortcut icon', href='/img/favicon.ico')
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
  body.minimal.error-page
    script.
      var boxtest = localStorage.getItem('boxed'); if (boxtest === 'true') {document.body.className+=' boxed-layout';}
    // Start: Header
    header.navbar.navbar-fixed-top
      .navbar-branding
        span#toggle_sidemenu_l.glyphicons.glyphicons-show_lines
        a.navbar-brand(href='/login')
          img(src='/img/logos/header-logo.png')
    // Start: Main
    #main
      #content.pn
        a#return-arrow(href='/')
          i.fa.fa-arrow-left.fa-3x.text-purple2
          span
            | Return 
            br
            |     to Theme
        h1.error-title  403! 
        h2.error-subtitle Forbidden! You are not allowed to perform this action
        .mid-section
          .mid-content
            input.form-control(type='text', placeholder='Ask me a question!', value='Ask me a question!')
            .pull-left
              img.img-responsive.mt10(src='/img/logos/logo-publish-white.png', alt='logo')
            .pull-right.mt20
              i.fa.fa-facebook-square.fs40.text-white.mr20
              i.fa.fa-twitter-square.fs40.text-white.mr20
              i.fa.fa-google-plus-square.fs40.text-white.mr20
              i.fa.fa-envelope-square.fs40.text-white
            .clearfix
        .old.stuff.hidden
          img.img-responsive(src='/img/logos/logo-publish.png', alt='logo')
          h3.mb50  A webful of video awesomeness 
          h1.mb50.pt30
            b.text-purple2 404
            |  - Oh no something went 
            b.text-purple2 Wrong
            |  here
    // End: Main
    // Google Map API
    script(type='text/javascript', src='http://maps.google.com/maps/api/js?sensor=true')
    // jQuery
    script(type='text/javascript', src='/vendor/jquery/jquery-1.11.1.min.js')
    script(type='text/javascript', src='/vendor/jquery/jquery_ui/jquery-ui.min.js')
    // Bootstrap
    script(type='text/javascript', src='/vendor/bootstrap/js/bootstrap.min.js')
    // Theme Javascript
    script(type='text/javascript', src='/js/utility/spin.min.js')
    script(type='text/javascript', src='/js/utility/underscore-min.js')
    script(type='text/javascript', src='/js/main.js')
    script(type='text/javascript', src='/js/ajax.js')
    script(type='text/javascript', src='/js/custom.js')
    script(type='text/javascript').
      jQuery(document).ready(function () {
      "use strict";
      // Init Theme Core
      Core.init();
      // Enable Ajax Loading
      Ajax.init();
      $('.icon-option-menu li a').click(function () {
      iconName = $(this).children('i').attr('class');
      $('.error-icon span').removeClass('animated fadeIn text-purple2').animate({
      opacity: 0
      }, 350,
      function() {
      $(this).removeClass().addClass('text-purple2 ' + iconName).animate({opacity: 1}, 350);
      });
      });
      });
